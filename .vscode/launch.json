{
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Electron Main",
        "cwd": "${workspaceFolder}/enjoy",
        "runtimeExecutable": "${workspaceFolder}/enjoy/node_modules/@electron-forge/cli/script/vscode.sh",
        "windows": {
          "runtimeExecutable": "${workspaceFolder}/enjoy/node_modules/@electron-forge/cli/script/vscode.cmd"
        },
        // runtimeArgs will be passed directly to your Electron application
        "runtimeArgs": [
          "start"
        ],
        "sourceMaps": true,
        // "outFiles": ["${cwd}/.vite/build/*.js"],
        "console": "integratedTerminal"
      },
      {
        "name": "Electron: Renderer",
        "type": "msedge",
        "request": "launch",
        "cwd": "${workspaceFolder}/enjoy",
        "url": "http://localhost:5173",
        "sourceMaps": true,
        // "outFiles": ["${workspaceFolder}/enjoy/.vite/build/*.js"],
        "runtimeExecutable": "${workspaceFolder}/enjoy/node_modules/@electron-forge/cli/script/vscode.sh",
        "windows": {
          "runtimeExecutable": "${workspaceFolder}/enjoy/node_modules/@electron-forge/cli/script/vscode.cmd"
        },
        // runtimeArgs will be passed directly to your Electron application
        "runtimeArgs": [
          "start"
        ],
        "outputCapture": "std",
        "port": 9222
      }
    ]
  }